{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["node", "express"],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": false
  }
}