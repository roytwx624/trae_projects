{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "(?!api).*\.(html|css|js|png|jpg|gif|svg|ico)$",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}