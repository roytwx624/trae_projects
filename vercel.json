{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/register",
      "dest": "api/register.js"
    },
    {
      "src": "/api/registrations",
      "dest": "api/registrations.js"
    },
    {
      "src": "/api/healthz",
      "dest": "api/healthz.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}